{
  "name": "json-to-code",
  "version": "0.1.0",
  "description": "JSON-to-Code Encoding: Compress JSON Data into Actual Code",
  "main": "json-to-code.js",
  "files": [
    "json-to-code.js"
  ],
  "scripts": {
    "build": "npx browserify -d -e json-to-code.js -o json-to-code.min.js -s jsonToCode && cp json-to-code.min.js ./demo/.",
    "demo": "npm run build && cd demo && npx srvd --debug",
    "format": "npx prettier --arrow-parens=avoid --print-width=240 --trailing-comma=none --write *.js",
    "setup": "cd test-data && bash setup.sh",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJDufour/json-to-code.git"
  },
  "keywords": [
    "code",
    "compression",
    "data",
    "encoding",
    "gram",
    "javascript",
    "json"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/json-to-code/issues"
  },
  "homepage": "https://github.com/DanielJDufour/json-to-code#readme",
  "dependencies": {
    "advarr": "^0.4.0",
    "declare-vars": "^0.1.0",
    "deconcat": "^0.0.1",
    "deep-counter": "^0.0.2",
    "deepest-walk": "^1.0.2",
    "flug": "^2.2.0",
    "min-quote": "*",
    "n-gram-counter": "*",
    "separo": "^0.0.2",
    "striptags": "^3.1.1",
    "textops": "^0.0.2",
    "var-names": "*"
  },
  "devDependencies": {
    "wkt-crs": "^0.1.1"
  }
}
